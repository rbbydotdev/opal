{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/download.zip",
      "destination": "/download.zip"
    },

    { "source": "/@static/(.*)", "destination": "/@static/$1" },
    {
      "source": "/auth/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*).js",
      "destination": "/$1.js"
    },
    {
      "source": "/workspace/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/preview/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/preview",
      "destination": "/index.html"
    },
    {
      "source": "/settings",
      "destination": "/index.html"
    },
    {
      "source": "/newWorkspace",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}
