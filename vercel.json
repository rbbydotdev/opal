{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/@static/(.*)", "destination": "/@static/$1" },

    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "Service-Worker-Allowed", "value": "/" }]
    }
  ]
}
